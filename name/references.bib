	author = {Wernberg, Thomas and Bennett, Scott and Babcock, Russell C. and de Bettignies, Thibaut and Cure, Katherine and Depczynski, Martial and Dufois, Francois and Fromont, Jane and Fulton, Christopher J. and Hovey, Renae K. and Harvey, Euan S. and Holmes, Thomas H. and Kendrick, Gary A. and Radford, Ben and Santana-Garcon, Julia and Saunders, Benjamin J. and Smale, Dan A. and Thomsen, Mads S. and Tuckett, Chenae A. and Tuya, Fernando and Vanderklift, Mathew A. and Wilson, Shaun},
	year = {2016},
	note = {bibtex*[publisher=American Association for the Advancement of Science;eprint=http://science.sciencemag.org/content/353/6295/169.full.pdf]},
	pages = {169--172}
}

